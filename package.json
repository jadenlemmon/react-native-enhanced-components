{
  "name": "react-native-enhanced-components",
  "version": "0.2.2",
  "description": "Enhanced React-Native components",
  "main": "src/index.js",
  "author": "Jaden Lemmon",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest"
  },
  "husky": {
